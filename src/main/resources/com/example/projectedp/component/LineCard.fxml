<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.projectedp.controller.LineCardController"
      spacing="10" style="-fx-padding: 10; -fx-border-color: #ccc; -fx-border-radius: 8; -fx-background-radius: 8; -fx-background-color: #f4f4f4;">

    <VBox spacing="5">
        <Label fx:id="lineNumberLabel" style="-fx-font-size: 20px; -fx-font-weight: bold;" />
        <Label fx:id="directionLabel" style="-fx-font-size: 14px;" />
    </VBox>
</HBox>

